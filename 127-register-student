public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c rec = new apxio__Student__c();
    rec.Name = name;
    rec.apxio__Phone__c = phone;
    rec.apxio__Email__c = email;
    insert rec;
    rec = [SELECT Name, apxio__Phone__c, apxio__Email__c, apxio__Registration_Number__c from apxio__Student__c WHERE Id= :rec.Id];
    return rec.apxio__Registration_Number__c;
}