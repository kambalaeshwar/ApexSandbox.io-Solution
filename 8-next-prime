public Integer nextPrime(Integer num) {
    While(true){
        Integer Count=0;
        Integer NP = ++num;
        for(Integer i = 2; i<NP; i++){
            If(Math.mod(NP,i)==0){
                Count++;
            }
        }
        if(Count==0){
            return NP;
        }
        }
    }
}